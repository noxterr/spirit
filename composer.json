{
    "name": "noxterr/spirit",
    "description": "A package that handles working with files with B2 (backblaze) in an elegant manner",
    "keywords": ["laravel", "backblaze", "b2", "files", "storage"],
    "homepage": "https://github.com/noxterr/spirit",
    "license": "MIT",
    "version": "2.0.0",
    "authors": [
        {
            "name": "Mirko Dal Fara",
            "email": "mirko.dalfarra@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/config": "^10.48.4|^11.0.8",
        "illuminate/support": "^11.26",
        "illuminate/console": "^11.26",
        "illuminate/database": "^11.28"
    },
    "autoload": {
        "psr-4": {
            "Noxterr\\Spirit\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Noxterr\\Spirit\\SpiritServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
